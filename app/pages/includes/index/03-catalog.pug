section.catalog
  .container
    .catalog-header
      h2.title.catalog-title Все квесты
      nav(data-tabs-titles).catalog-tab-nav
        button.catalog-tab-nav__item._active(data-tabs-button="quest" data-title="Все квесты") Квесты
        button.catalog-tab-nav__item(data-tabs-button="arena"  data-title="Все VR арены") VR арена
    .catalog-tabs-parent
      .catalog-tab._active#quest
        .catalog-navbar
          .catalog-category
            ul.catalog-category__list
              li.catalog-category__item.current-category
                a(href="#") Для детей
              li.catalog-category__item
                a(href="#") Для взрослых
              li.catalog-category__item
                a(href="#") Логические
              li.catalog-category__item
                a(href="#") Страшные
              li.catalog-category__item
                a(href="#") Детский ДР
          .catalog-sorting(data-da=".catalog-filters__form, 1023, first, ")
            label(for="sorting") Сортировка
            select#sorting(name="sorting")
              option(value="По популярности" selected) По популярности
              option(value="Новинки") Новинки
              option(value="Сначала дешёвые") Сначала дешёвые
              option(value="Сначала дорогие") Сначала дорогие
          .catalog-filters
            button.catalog-filters__btn
            form.catalog-filters__form
              .catalog-filters__item
                label(for="genre") Жанр
                select#genre(name="genre")
                  option(value="Все жанры" selected) Все жанры
                  option(value="Для детей") Для детей
                  option(value="Для взрослых") Для взрослых
                  option(value="Логические") Логические
                  option(value="Детский ДР") Детский ДР
              .catalog-filters__item
                label(for="players") Количество игроков
                select#players(name="players")
                  option(value="2" selected) 2
                  option(value="4") 4
                  option(value="8") 8
                  option(value="12") 12
                  option(value="16") 16
                  option(value="20+") 20+
              .catalog-filters__item
                label(for="age") Возраст, лет
                select#age(name="age")
                  option(value="от 6 до 12" selected) от 6 до 12
                  option(value="от 12 до 16") от 12 до 16
                  option(value="от 18") от 18
              .catalog-filters__item
                label(for="age") Цена, руб
                .catalog-filters__row
                  input(type="number" id="age" placeholder="1000" min="1000" max="5000")
                  span.line -
                  input(type="number" placeholder="50 000" min="1000" max="50000")
              .catalog-filters__footer
                button.catalog-filters__clear(type="reset") Очистить фильтр
                button.btn.catalog-filters__submit(type="submit") Найти
        .catalog-body
          include 03-catalog-card
          include 03-catalog-card
          include 03-catalog-card
          include 03-catalog-card
          include 03-catalog-card
          include 03-catalog-card
          include 03-catalog-card
          include 03-catalog-card
          include 03-catalog-card
        .catalog-footer
          .catalog-footer__info Показано: 9 – 18 из 124
          btn.btn.btn-outline.catalog-footer__btn Показать ещё
          ul.catalog-footer__pagination.pagination
            li.pagination__item
              a(href="#")
                <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" clip-rule="evenodd" d="M12.9067 3.69222C13.1688 3.91686 13.1992 4.31143 12.9745 4.5735L8.32317 10.0001L12.9745 15.4267C13.1992 15.6888 13.1688 16.0833 12.9067 16.308C12.6447 16.5326 12.2501 16.5022 12.0255 16.2402L7.02546 10.4068C6.82485 10.1728 6.82485 9.8274 7.02546 9.59335L12.0255 3.76001C12.2501 3.49794 12.6447 3.46759 12.9067 3.69222Z" />
